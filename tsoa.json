{
  "entryFile": "src/index.ts",
  "controllerPathGlobs": [
    "src/**/*.controller.ts"
  ],
  "spec": {
    "basePath": "/api",
    "outputDirectory": "./api/dist",
    "specVersion": 3
  },
  "routes": {
    "basePath": "/api",
    "entryFile": "./src/index.ts",
    "routesDir": "./src/routes",
    "middleware": "express",
    "authenticationModule": "./src/authentication.ts"
  }
}